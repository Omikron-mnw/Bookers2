<% @books.each do |book| %>
<table>
   <!--attachment_tag?-->
  <%= attachment_image_tag book.user, :profile_image, :fill, 30, 30, fallback: "no_image.jpg" %>
  <p>user_name : <%= book.user.name %></p>
  <p>title : <%= book.title %></p>
  <p>opinion : <%= book.opinion %></p>
</table>
<% end %>
<%= paginate @books %>