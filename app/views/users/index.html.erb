<% @users.each do |book| %>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <p><%= attachment_image_tag @user_image, :profile_image, :fill, 30, 30, fallback: "no_image.jpg" %></p>
  <p><%= @user_name %></p>
  <p><%= link_to "Show", @user_path %></p>
</table>
<% end %>
<%= paginate @users %>